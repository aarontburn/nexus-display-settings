{
    "name": "Display Settings",
    "id": "aarontburn.Display_Settings",
    "author": "aarontburn",
    "version": "1.0.0",
    "description": "Adds a few display setting to Nexus.",
    "platforms": [
        "win32",
        "darwin",
        "linux"
    ],
    "link": "https://github.com/aarontburn/nexus-display-settings",
    "git-latest": {
        "git-username": "aarontburn",
        "git-repo-name": "nexus-display-settings"
    },
    "author-id": "683193056e5989838dcf772c",
    "build": {
        "build-version": 2,
        "excluded": [
            "electron.ts"
        ],
        "included": [],
        "process": "./process/main.js",
        "replace": [
            {
                "from": "{EXPORTED_MODULE_ID}",
                "to": "%id%",
                "at": [
                    "./process/main.ts"
                ]
            },
            {
                "from": "{EXPORTED_MODULE_NAME}",
                "to": "%name%",
                "at": [
                    "./process/main.ts"
                ]
            }
        ]
    }
}